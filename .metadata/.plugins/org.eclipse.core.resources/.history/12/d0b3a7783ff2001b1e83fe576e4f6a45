package algoritmos.exemplos;

public class ProcessamentoDeDadosAula25 {

	public static void main(String[] args) {

		//Exemplo 1
		int x = 5, y = 2 * x;
		System.out.println(x);

		//Exemplo 2
		int x1 = 5;
		double y1 = 2 * x;
		System.out.println(x1);
		System.out.println(y1);
		
		//Exemplo 3
		float b = 6f, B = 8f, h = 5f, area;
		area = (b+B) / 2f * h;
		System.out.println(area);
		
		//Exemplo 4
		int a1 = 5, b1 = 2;
		double resultado = a1 / b1;
		System.out.println(resultado);
		
		//Exemplo 5
		double a2 = 5.0;
		int b2;
		b2 = (int) a2;
		System.out.println(b2);
		
	}

}
