package algoritmos;

public class Main {

	public static void main(String[] args) {
		
		double x = 10.35784;
		
		System.out.println(x);
		
		//mascara para delimitar casas decimais
		System.out.printf("%.2f%n", x);
		
		//mascara para delimitar casas decimais 
		System.out.printf("%.4f%n", x);
	}

}
